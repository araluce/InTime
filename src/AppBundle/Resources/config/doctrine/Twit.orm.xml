<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="AppBundle\Entity\Twit" table="TWIT">
    <indexes>
      <index name="id_twittero" columns="id_twittero"/>
    </indexes>
    <id name="idTwit" type="integer" column="id_twit">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="idTwittero" type="string" column="id_twittero" length="30" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="fecha" type="datetime" column="fecha" nullable="false"/>
  </entity>
</doctrine-mapping>
