<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="AppBundle\Entity\AlbumFoto" table="ALBUM_FOTO">
    <indexes>
      <index name="id_usuario" columns="id_usuario"/>
    </indexes>
    <id name="idAlbumFoto" type="integer" column="id_album_foto">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="fecha" type="datetime" column="fecha" nullable="false"/>
    <field name="imagen" type="string" column="imagen" length="1000" nullable="true"/>
    <many-to-one field="idUsuario" target-entity="Usuario">
      <join-columns>
        <join-column name="id_usuario" referenced-column-name="id_usuario"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>
